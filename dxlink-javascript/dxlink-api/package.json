{
  "name": "@dxfeed/dxlink-api",
  "version": "0.1.2",
  "private": false,
  "sideEffects": false,
  "description": "dxLink API",
  "source": "src/index.ts",
  "exports": {
    "types": "./build/index.d.ts",
    "require": "./build/index.js",
    "default": "./build/index.module.js"
  },
  "main": "build/index.js",
  "module": "build/index.module.js",
  "types": "build/index.d.ts",
  "browserslist": [
    "last 1 year"
  ],
  "minify": {
    "mangle": false
  },
  "scripts": {
    "start": "vite ./demo",
    "prepublishOnly": "npm run build",
    "build": "microbundle -f esm,cjs",
    "lint": "eslint src"
  },
  "dependencies": {
    "@dxfeed/dxlink-core": "0.1.2",
    "@dxfeed/dxlink-websocket-client": "0.1.2",
    "@dxfeed/dxlink-feed": "0.1.2"
  },
  "author": "Dmitry Petrov <dmitry.petrov@devexperts.com>",
  "maintainers": [
    {
      "name": "Fyzu",
      "url": "https://github.com/fyzu"
    }
  ],
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dxfeed/dxlink.git"
  },
  "bugs": {
    "url": "https://github.com/dxfeed/dxlink/issues"
  },
  "homepage": "https://dxfeed.com/",
  "files": [
    "/build",
    "/package.json"
  ],
  "keywords": [
    "api",
    "feed",
    "client",
    "financial-data",
    "market-data",
    "market-data-feeds",
    "market-api",
    "stock-market",
    "stock-price-data",
    "real-time-data",
    "historical-data",
    "dxfeed",
    "dxlink"
  ]
}
